DISABLE_API_KEY_AUTH=true
STORAGE_BACKEND=local
LOCAL_STORAGE_ROOT=/absolute/path/to/local-storage
QWEN_TTS_MODEL_ID=Qwen/Qwen3-TTS-12Hz-0.6B-CustomVoice
QWEN_TTS_MODEL_MODE=custom_voice
# If you choose a Base model but do not configure reference audio, API falls back to CustomVoice:
QWEN_TTS_AUTO_FALLBACK_FROM_BASE=true
# Optional: if UI asks for an unavailable voice id, fallback to first supported speaker:
QWEN_TTS_ALLOW_UNKNOWN_SPEAKER_FALLBACK=true
QWEN_TTS_TOKENIZER_ID=Qwen/Qwen3-TTS-Tokenizer-12Hz
# Base mode only (voice cloning):
# QWEN_TTS_BASE_REF_AUDIO=/absolute/path/to/reference_voice.wav
# QWEN_TTS_BASE_REF_TEXT=Hello, this is my reference voice.
# Optional: map frontend voice ids to reference audios (base mode):
# QWEN_TTS_VOICE_CLONE_PRESETS={"Cherry":{"ref_audio":"/path/cherry.wav","ref_text":"Hello from Cherry"}}
QWEN_TTS_HOST=0.0.0.0
QWEN_TTS_PORT=8003
